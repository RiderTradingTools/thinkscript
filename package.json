{
  "name": "thinkscript",
  "displayName": "thinkscript",
  "description": "thinkscript syntax highlighting used in thinkorswim trading platform",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "thinkscript",
      "aliases": ["thinkscript", "thinkscript"],
      "extensions": [".thinkscript"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "thinkscript",
      "scopeName": "source.thinkscript",
      "path": "./syntaxes/thinkscript.tmLanguage.json"
    }]
  }
}
